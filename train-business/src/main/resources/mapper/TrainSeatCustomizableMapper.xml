<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zjl.train.business.mapper.TrainSeatCustomizableMapper">

    <select id="selectByUnique" resultType="com.zjl.train.business.entity.TrainSeat">
        SELECT *
        FROM train_seat
        WHERE name = #{name}
    </select>
</mapper>